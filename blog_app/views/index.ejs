<%- include('./partials/header') %>
<h1>Index page</h1>
<% blogs.forEach( (blog) => {%>
<div>
    <h2><%= blog.title %></h2>
    <img src="<%= blog.image %>" alt="<%= blog.title %>"/>
    <span><%= blog.create %></span>
    <p><%= blog.body.substring(0,125) %> ... </p>
    <a href="/blogs/<%=blog._id%>">Read More...</a>
</div>


<% });%>
<%- include('./partials/footer') %>